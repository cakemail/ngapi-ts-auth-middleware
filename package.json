{
  "name": "@cakemail/ngapi-ts-auth-middleware",
  "version": "1.0.0",
  "description": "Express TypeScript authentication/authorization middleware for Cakemail's API",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run format:check && npm run build && npm test"
  },
  "keywords": [
    "express",
    "middleware",
    "authentication",
    "authorization",
    "jwt",
    "typescript",
    "ngapi",
    "cakemail",
    "redis",
    "bearer-token"
  ],
  "author": "Cakemail",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cakemail/ngapi-ts-auth-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/cakemail/ngapi-ts-auth-middleware/issues"
  },
  "homepage": "https://github.com/cakemail/ngapi-ts-auth-middleware#readme",
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ioredis-mock": "^8.13.1",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
